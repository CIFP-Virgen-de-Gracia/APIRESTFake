{
  "name": "apirestfake",
  "version": "1.0.2",
  "description": "Fake API REST para proyectos para praticar y consumir datos de ella",
  "main": "src/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "server": "node --require @babel/register src/server.js",
    "lint": "eslint 'src/**/*.js'",
    "fix": "eslint --fix 'src/**/*.js'",
    "watch": "nodemon",
    "prune": "npm prune --production",
    "undist": "rm -rf dist",
    "build": "npm run undist && babel src -d dist && cp -r 'public' 'dist/public' && cp -r 'src/bd' 'dist/bd'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joseluisgs/APIRESTFake.git"
  },
  "keywords": [
    "API",
    "REST"
  ],
  "author": "joseluisgs",
  "license": "MIT",
  "twitter": "https://twitter.com/joseluisgonsan",
  "bugs": {
    "url": "https://github.com/joseluisgs/APIRESTFake/issues"
  },
  "homepage": "https://github.com/joseluisgs/APIRESTFake#readme",
  "dependencies": {
    "json-server": "^0.16.2",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-preset-airbnb": "^5.0.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.6"
  }
}
